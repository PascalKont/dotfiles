# Mobile
profile Mobile {
    output $INTERNAL enable
  }

# HOME
profile Home {
    output $INTERNAL position 0,900 enable
    output $HOME enable
#    exec swaymsg workspace 0, move workspace to output '"Samsung Display Corp. 0x419F Unknown"'
#    exec swaymsg workspace 1, move workspace to output '"Ancor Communications Inc ROG PG348Q G9LMQS137390"'
  }

# MUC1, hinten links mittig
profile MUC1 {
    output $MUC1_left enable
    output $MUC1_right enable
    output $INTERNAL enable
#    exec swaymsg workspace 0, move workspace to output '"Samsung Display Corp. 0x419F Unknown"'
#    exec swaymsg workspace 1, move workspace to output '"NEC Corporation E243F 25601651HB"'
#    exec swaymsg workspace 2, move workspace to output '"NEC Corporation E243F 25601685HB"'
  }

# MUC_WC
profile MUC_WC {
  output $MUC_wc_left enable
  output $MUC_wc_right enable
  output $INTERNAL enable
  exec swaymsg workspace 0, move workspace to output '"Samsung Display Corp. 0x419F Unknown"'
  exec swaymsg workspace 1, move workspace to output '"NEC Corporation E243F 25601585HB"'
  exec swaymsg workspace 2, move workspace to output '"NEC Corporation E243F 25601658HB"'
  }

# MUC2, hinten links an vorderer wand
profile MUC2 {
    output $MUC2_left enable
    output $MUC2_right enable
    output $INTERNAL position 3840,800 enable
#    exec swaymsg workspace 0, move workspace to output '"NEC Corporation E243F 25601662HB"'
#    exec swaymsg workspace 1, move workspace to output '"NEC Corporation E243F 25601668HB"'
#    exec swaymsg workspace 2, move workspace to output '"Samsung Display Corp. 0x419F Unknown"'
  }

profile MUC3 {
    output $MUC3_left enable
    output $MUC3_right enable
    output $INTERNAL position 3840,800 enable
  }

# WB1
profile WB1 {
    output $WB1_left enable
    output $WB1_right enable
    output $INTERNAL position 3840,400 enable
  }

# WB2
profile WB2 {
    output $WB2_left enable
    output $WB2_right enable
    output $INTERNAL position 0,400 enable
  }

