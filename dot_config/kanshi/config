output 'Samsung Display Corp. 0x419F Unknown' {
  mode 2880x1800@120.000
  position 0,0
  alias $INTERNAL
  }

output 'Ancor Communications Inc ROG PG348Q G9LMQS137390' {
    mode 3440x1440@49.987
    position 1440,0
    alias $HOME
  }

output 'NEC Corporation E243F 25601651HB' {
    mode 1920x1080@60.000 
    position 1440,0
    alias $MUC1_left
  }

output 'NEC Corporation E243F 25601685HB' {
    mode 1920x1080@60.000
    position 3360,0
    alias $MUC1_right
  }

